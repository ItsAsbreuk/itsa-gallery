YUI(yuiconfig).use('gallery-itsastatusbar', 'gallery-itsadialog', function(Y) {

    var mystatusbar = new Y.ITSAStatusbar();
    mystatusbar.handleLevel('info');
    mystatusbar.handleLevel('warn');
    mystatusbar.handleLevel('error');

    Y.showMessage('title info', 'Message info');
    Y.alert('title warning', 'message warning');
    Y.showError('title error', 'message error');

    Y.confirm('Y.confirm', 'do you agree?').then(
        function() {
            console.log('agreed');
        },
        function() {
            console.log('disagreed');
        }
    );


});
